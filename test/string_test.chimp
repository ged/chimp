use chimpunit;

main argv {
  chimpunit.test("string escapes", fn { |t|
    t.equals("abcdef\n".size(), 7);
  });
  chimpunit.test("newline in string", fn { |t|
    t.equals("abcdef
foo", "abcdef\nfoo");
  });
  chimpunit.test("concatenation with `+`", fn { |t|
    t.equals("foo" + "bar", "foobar");
  });
}

